{
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/lodash": "^4.14.201",
        "prisma": "^5.5.2",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    },
    "name": "skeet-discord-bot",
    "description": "Posts your bluesky skeets to Discord",
    "version": "1.0.0",
    "main": "src/main.ts",
    "scripts": {
        "build": "tsc",
        "build:prod": "rm -rf ./dist/ && npm run db:generate && npm run build",
        "start": "node dist/main.js",
        "dev": "ts-node src/main.ts",
        "db:generate": "prisma generate",
        "db:migrate": "prisma migrate dev",
        "db:push": "prisma migrate deploy",
        "register-commands": "ts-node src/scripts/register_commands.ts"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@atproto/api": "^0.6.23",
        "@atproto/identity": "^0.3.1",
        "@atproto/repo": "^0.3.4",
        "@atproto/xrpc-server": "^0.4.0",
        "@maljs/bsky-helpers": "^1.0.4",
        "@prisma/client": "5.5.2",
        "discord.js": "^14.13.0",
        "dotenv": "^16.3.1",
        "lodash": "^4.17.21",
        "lru-cache": "^10.0.1",
        "multiformats": "^12.1.3",
        "pino": "^8.16.1"
    },
    "volta": {
        "node": "20.9.0"
    }
}
